{
  "expo": {
    "name": "Up Partner",
    "slug": "up-partner",
    "version": "1.0.0",
    "platforms": ["ios", "android"],

    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "in.upstore.uppartner",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "in.upstore.up_partner",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
